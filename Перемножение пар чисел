#include <iostream>
#include <vector>

int main() {
    // Запрос количества пар чисел у пользователя
    std::cout << "Введите количество пар чисел: ";
    int n;
    std::cin >> n;

    // Вектор для хранения пар чисел
    std::vector<std::pair<int, int>> pairs;

    // Ввод пар чисел от пользователя
    for (int i = 0; i < n; ++i) {
        int x, y;
        std::cout << "Введите " << i + 1 << "-ую пару чисел: ";
        std::cin >> x >> y;
        pairs.push_back(std::make_pair(x, y));
    }

    // Инициализация результатов перемножения
    int resultX = 1;
    int resultY = 1;

    // Перемножение всех пар чисел
    for (const auto& pair : pairs) {
        resultX *= pair.first;
        resultY *= pair.second;
    }

    // Вывод результата
    std::cout << "Результат перемножения: (" << resultX << ", " << resultY << ")\n";

    return 0;
}
