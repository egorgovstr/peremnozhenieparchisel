#include <iostream>
#include <sstream>
#include <string>
#include <vector>

using namespace std;

// Функция для перемножения пар чисел
string multiplyPair(const string& num1, const string& num2) {
    // Проверка наличия второго числа в паре
    if (num2 == "")
        return "0";
    
    // Проверка наличия символа в числе
    if (num1.find_first_of("abcdefghijklmnopqrstuvwxyz") != string::npos ||
        num2.find_first_of("abcdefghijklmnopqrstuvwxyz") != string::npos) {
        return num1 + num2;
    }
    
    // Выполнение перемножения чисел
    int n1 = stoi(num1);
    int n2 = stoi(num2);
    int result = n1 * n2;
    
    return to_string(result);
}

int main() {
    string input;
    cout << "Введите пары чисел и символов: ";
    getline(cin, input);
    
    // Разделение ввода на пары
    istringstream iss(input);
    vector<string> pairs;
    string pair;
    while (iss >> pair) {
        pairs.push_back(pair);
    }
    
    // Перемножение пар и вывод результата
    for (int i = 0; i < pairs.size(); i += 2) {
        string result = multiplyPair(pairs[i], pairs[i+1]);
        cout << result << ", ";
    }

    return 0;
}
